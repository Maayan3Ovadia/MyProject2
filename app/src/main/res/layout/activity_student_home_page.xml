<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E1BCCAF3"
    android:visibility="visible"
    tools:context=".StudentHomePage">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="עמוד אישי"
        android:textColor="#0B0B0B"
        android:textSize="42sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="מספר שיעורים שעשיתי:"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="205dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="שם המורה:"
        android:textColor="#050505"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="301dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="השיעורים הבאים:"
        android:textColor="#0E0E0E"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="353dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="שיעורים קודמים"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="250dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="לקביעת שיעור"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="398dp" />

</androidx.constraintlayout.widget.ConstraintLayout>